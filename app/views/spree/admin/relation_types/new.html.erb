<% content_for :page_title do %>
  <%= link_to Spree.t(:relation_types), spree.admin_relation_types_url %> /
  <%= Spree.t(:new_relation_type) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @relation_type } %>

<%= form_for :relation_type, url: collection_url do |f| %>
  <fieldset>
    <%= render 'form', f: f %>
    <%= render 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
